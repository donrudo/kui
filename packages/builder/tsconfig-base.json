{
  "include": [],
  "exclude": ["**/flycheck*.ts"],
  "compilerOptions": {
    "removeComments": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "ES6",
    "skipLibCheck": true,
    "noEmitOnError": false,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "lib": ["dom", "esnext", "es6", "dom.iterable", "scripthost"],
    "baseUrl": ".",
    "paths": {
      "@kui-shell/core": ["../../packages/core/src"],
      "@kui-shell/core/*": ["../../packages/core/src/*"],
      "@kui-shell/core/mdist/*": ["../../packages/core/src/*"],
      "@kui-shell/test": ["../../packages/test/src"],
      "@kui-shell/test/*": ["../../packages/test/src/*"],
      "@kui-shell/react": ["../../packages/react/src"],
      "@kui-shell/react/*": ["../../packages/react/src/*"],
      "@kui-shell/client": ["../../plugins/plugin-client-default/src"],
      "@kui-shell/client/*": ["../../plugins/plugin-client-default/src/*"],
      "@kui-shell/plugin-bash-like": ["../../plugins/plugin-bash-like/src"],
      "@kui-shell/plugin-bash-like/*": ["../../plugins/plugin-bash-like/*/src"],
      "@kui-shell/plugin-client-common": ["../../plugins/plugin-client-common/src"],
      "@kui-shell/plugin-client-common/*": ["../../plugins/plugin-client-common/*/src"],
      "@kui-shell/plugin-client-common/mdist/*": ["../../plugins/plugin-client-common/src/*"],
      "@kui-shell/plugin-client-common/notebook": ["../../plugins/plugin-client-common/notebook/src"],
      "@kui-shell/plugin-core-support": ["../../plugins/plugin-core-support/src"],
      "@kui-shell/plugin-core-support/*": ["../../plugins/plugin-core-support/*/src"],
      "@kui-shell/plugin-git": ["../../plugins/plugin-git/src"],
      "@kui-shell/plugin-kubectl": ["../../plugins/plugin-kubectl/src"],
      "@kui-shell/plugin-kubectl/*": ["../../plugins/plugin-kubectl/*/src"],
      "@kui-shell/plugin-kubectl/components/mdist/*": ["../../plugins/plugin-kubectl/components/src/*"],
      "@kui-shell/plugin-kubectl-core": ["../../plugins/plugin-kubectl-core/src"],
      "@kui-shell/plugin-kubectl-tray-menu": ["../../plugins/plugin-kubectl-tray-menu/src"],
      "@kui-shell/plugin-madwizard": ["../../plugins/plugin-madwizard/src"],
      "@kui-shell/plugin-madwizard/watch": ["../../plugins/plugin-madwizard/watch/src"],
      "@kui-shell/plugin-madwizard/do": ["../../plugins/plugin-madwizard/do/src"],
      "@kui-shell/plugin-madwizard/*": ["../../plugins/plugin-madwizard/*/src"],
      "@kui-shell/plugin-proxy-support": ["../../plugins/plugin-proxy-support/src"],
      "@kui-shell/plugin-s3": ["../../plugins/plugin-s3/src"],
      "@kui-shell/plugin-s3/*": ["../../plugins/plugin-s3/*/src"]
    }
  }
}
